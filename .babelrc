{
    "presets": [
      "@babel/typescript",
      [
        "@babel/preset-env",
        {
          // "useBuiltIns": "usage",
          "targets":{
            "ie":10
          }
          // "corejs": {
          //   "version": 3,
          //   "proposals": true // 使用尚在“提议”阶段特性的 polyfill
